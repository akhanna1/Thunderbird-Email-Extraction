{
	"author": "CMU-SBO Research Team",
	"name": "DEV: SBO Research Study Extension",
	"version": "10.1.0",
	"icons": {
		"128": "icon.jpg"
	},
	"background": {
		"scripts": [
			"libraries/abp/PublicSuffixList.js",
			"libraries/abp/TldUtility.js",
			"libraries/abp/CommonUtility.js",
			"libraries/abp/CoreUtility.js",
			"libraries/abp/UrlUtility.js",
			"libraries/abp/Filter.js",
			"libraries/abp/CommentFilter.js",
			"libraries/abp/ActiveFilter.js",
			"libraries/abp/RegExpFilter.js",
			"libraries/abp/BlockingFilter.js",
			"libraries/abp/ElemHideBase.js",
			"libraries/abp/ElemHideException.js",
			"libraries/abp/ElemHideEmulationFilter.js",
			"libraries/abp/ElemHideFilter.js",
			"libraries/abp/InvalidFilter.js",
			"libraries/abp/WhitelistFilter.js",
			"libraries/abp/Matcher.js",
			"libraries/abp/CombinedMatcher.js",
			"libraries/abp/Subscription.js",
			"libraries/abp/RegularSubscription.js",
			"libraries/abp/SpecialSubscription.js",
			"libraries/abp/DownloadableSubscription.js",
			"libraries/abp/ExternalSubscription.js",
			"libraries/abp/FrameCache.js",
			"libraries/abp/RequestFilterFacade.js",
			"libraries/jsencrypt.min.js",
			"libraries/bcrypt.min.js",
			"libraries/aes.js",
			"libraries/enc-base64-min.js",
			"libraries/sjcl.js",
			"libraries/lz-string.min.js",
			"libraries/fingerprint2.min.js",
			"libraries/is.min.js",
			"libraries/lru.js",
			"GUIDs.js",
			"Constants.js",
			"EncryptionUtility.js",
			"EntropyUtility.js",
			"LanguageUtilities.js",
			"Observation.js",
			"Protects.js",
			"SubStrings.js",
			"BrowserIdentity.js",
			"BrowserSalt.js",
			"Context.js",
			"StorageFacade.js",
			"LocalStore.js",
			"GlobalContext.js",
			"ContextFactory.js",
			"ModuleStatus.js",
			"ModuleStatusController.js",
			"MessageTypes.js",
			"MessageConsumer.js",
			"PortConsumer.js",
			"SendCommand.js",
			"ObservationSender.js",
			"ObservationCache.js",
			"ServerAPI.js",
			"KeyServer.js",
			"Server.js",
			"SafeBrowsingAPI.js",
			"SafeBrowsing.js",
			"nn-client.min.js",
			"NeuralNetworkFacade.js",
			"NeuralNetworkLib.js",
			"MessageDecorator.js",
			"ObservationFactory.js",
			"ObservationProtection.js",
			"Observations.js",
			"modules/WebNavigation.js",
			"modules/Management.js",
			"modules/Downloads.js",
			"modules/Cookies.js",
			"modules/Tabs.js",
			"modules/History.js",
			"modules/Languages.js",
			"modules/Runtime.js",
			"modules/WebRequests.js",
			"modules/ContentSettings.js",
			"modules/Privacy.js",
			"main_background.js"
		],
		"persistent": true
	},
	"content_scripts": [{
			"matches": [
				"http://*/*", 
				"https://*/*", 
				"<all_urls>"
			],
			"js": [
				"libraries/jsencrypt.min.js",
				"libraries/bcrypt.min.js",
				"libraries/aes.js",
				"libraries/enc-base64-min.js",
				"libraries/sjcl.js",
				"GUIDs.js",
				"Constants.js",
				"EncryptionUtility.js",
				"EntropyUtility.js",
				"LanguageUtilities.js",
				"InputEvent.js",
				"InputEventMap.js",
				"Observation.js",
				"ModuleStatus.js",
				"MessageTypes.js",
				"ContentScriptLogging.js",
				"MessageFactory.js",
				"MessageProducer.js",
				"MessageDecorator.js",
				"ContentScriptObservations.js",
				"main_content.js"
			],
			"all_frames": true,
			"match_about_blank": true
		}, {
			"matches": [
				"https://sbo.cs.cmu.edu/*"
			],
			"js": [
				"GUIDs.js",
				"ContentScriptLogging.js",
				"participant_content.js"
			]
		}
	],
	"permissions": [
		"background",
		"storage",
		"webNavigation",
		"management",
		"downloads",
		"cookies",
		"tabs",
		"history",
		"webRequest",
		"https://sbo.cs.cmu.edu/",
		"https://sbo.cs.cmu.edu/*",
		"<all_urls>",
		"browsingData",
		"bookmarks",
		"contentSettings",
		"privacy",
		"proxy"
	],
	"incognito": "spanning",
	"options_ui": {
		"page": "options.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"manifest_version": 2,
	"description": "This extension is intended for research purposes only and is a part of the Security Behavior Observatory study"
}
